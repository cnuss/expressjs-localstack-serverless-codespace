{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug",
      "program": "${workspaceRoot}/node_modules/serverless/bin/serverless",
      "args": ["offline", "start", "--noTimeout"],
      "outFiles": ["${workspaceFolder}/.webpack/**/*.js"],
      "sourceMaps": true,
      "internalConsoleOptions": "openOnSessionStart",
      "presentation": {
        "group": "Run",
        "hidden": false,
        "order": 1
      }
    }
  ]
}
